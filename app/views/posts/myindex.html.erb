<div class="row">
  <div class="col-md-1"></div>
  <div class="col-md-2"><h4><u>Title</u></h4></div>
  <div class="col-md-7"><h4><u>Description</u></h4></div>
  <div class="col-md-2"><h4><u>Action</u></h4></div>
</div>
<% @post.each do |post| %>
<div class="row">
  <div class="col-md-1"></div>
   <% if user_signed_in? %>
     <div class="col-md-2"><%= post.title %></div>
     <div class="col-md-7"><%= post.description %></div>
     <div class="col-md-2">
             <strong><%= link_to 'Show', post_path(post),remote: true %></strong>
             <strong><%= link_to 'Edit', edit_post_path(post) %></strong>
        </div>
   <%end%>
</div>


<div class="container">
  <div class="row voffset2">
    <div class="col-md-12">
      <p>
           
      </p>
    </div>
  </div>
</div>
<%end%>
<div class='text-center'>
</div>


